{
  "name": "fe-ui",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start": "NODE_ENV=production rollup --config rollup.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/react": "^1.3.3",
    "@chakra-ui/theme": "^1.6.2",
    "@chakra-ui/theme-tools": "^1.0.4",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@loadable/babel-plugin": "^5.13.2",
    "@spothero/babel-preset-spothero": "^0.2.0",
    "framer-motion": "^3.7.0",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@loadable/webpack-plugin": "^5.14.2",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@spothero/eslint-config": "^2.1.0",
    "@spothero/stylelint-config": "^2.1.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0"
  },
  "babel": {
    "presets": [
      "@spothero/spothero"
    ],
    "plugins": [
      "@loadable/babel-plugin",
      "react-hot-loader/babel"
    ]
  },
  "stylelint": {
    "extends": [
      "@spothero/stylelint-config",
      "stylelint-prettier/recommended"
    ],
    "ignoreFiles": [
      "src/sass/mobile.desktop.module.scss",
      "src/sass/vendor/_react-big-calendar.scss"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@spothero",
      "plugin:prettier/recommended",
      "prettier/babel",
      "prettier/react"
    ],
    "overrides": [
      {
        "files": [
          "**/*.js"
        ],
        "rules": {
          "func-names": 0
        }
      },
      {
        "files": [
          "**/*.{js,jsx}"
        ],
        "rules": {
          "no-console": "off"
        }
      }
    ]
  }
}
